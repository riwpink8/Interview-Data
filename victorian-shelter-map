<!DOCTYPE html>
<html>
<head>
  <title>My Vega-Lite Map</title>
  <script src="https://cdn.jsdelivr.net/npm/vega@5"></script>
  <script src="https://cdn.jsdelivr.net/npm/vega-lite@5"></script>
  <script src="https://cdn.jsdelivr.net/npm/vega-embed@6"></script>
</head>
<body>
  <div id="vis"></div>
  <script type="text/javascript">
    var spec = {
      "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
      "description": "A map of Victoria with number of animals in shelters.",
      "data": {
        "url": "https://raw.githubusercontent.com/riwpink8/animal-pound-data/refs/heads/main/Data%20for%20Week%209%20Homework.csv"
      },
      "mark": "bar",
      "encoding": {
        "x": {"field": "Category", "type": "nominal"},
        "y": {"field": "Amount", "type": "quantitative"}
      }
    };
    vegaEmbed('#vis', spec);
  </script>
</body>
</html>
